<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text layoutX="18.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Input the neccessary information:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="CarId" layoutX="186.0" layoutY="86.0" />
      <Text layoutX="18.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The id of the car:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Text layoutX="18.0" layoutY="151.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The planned distance:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="distance" layoutX="186.0" layoutY="132.0" />
      <Text layoutX="18.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="The number of persons:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="noOfPersons" layoutX="186.0" layoutY="187.0" />
      <Button fx:id="SumbitReserv" layoutX="431.0" layoutY="282.0" mnemonicParsing="false" onAction="#createReservNow" text="Sumbit" />
      <Button fx:id="Submit" layoutX="58.0" layoutY="282.0" mnemonicParsing="false" onAction="#GoToHome" text="Back" />
   </children>
</AnchorPane>
